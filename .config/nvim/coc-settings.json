{
  // "diagnostic.enableSign": false,
  "diagnostic.warningSign": ">>",
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": ["javascript"],
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "json",
    "css",
    "less",
    "html"
  ],
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnType": true,
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit",
    "<C-p>": "action:preview",
    "<C-j>": "do:next",
    "<C-k>": "do:previous",
    "<C-s>": "do:toggle",
    "<C-f>": "do:switch",
    "<A-x>": "do:toggleMode",
    "<Down>": "prompt:next",
    "<Up>": "prompt:previous",
    "<C-v>": "prompt:paste"
  },
  "cursors.cancelKey": "<C-c>"
}
